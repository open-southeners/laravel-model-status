{
    "name": "open-southeners/laravel-model-status",
    "description": "A very simple yet very integrated Laravel status package",
    "license": "MIT",
    "keywords": [
        "open-southeners"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "me@d8vjork.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9",
        "open-southeners/laravel-helpers": "dev-main"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelModelStatus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelModelStatus\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSoutheners\\LaravelModelStatus\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
