{
    "name": "open-southeners/laravel-model-status",
    "description": "A very simple yet very integrated Laravel status package",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "laravel",
        "laravel-package",
        "model-statuses",
        "php8",
        "php-attributes",
        "php-enums"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "me@d8vjork.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0 || ^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.0 || ^10.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelModelStatus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelModelStatus\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
